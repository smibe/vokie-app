<script>
    function pausieren(ele1)
    {
       document.getElementById(ele1.id).pause();
       document.getElementById("playbutton").style.display = "";
       document.getElementById("pausebutton").style.display = "none";
    }

   function abspielen(ele1)

    {
       document.getElementById(ele1.id).play();

       document.getElementById("pausebutton").style.display = "";

       document.getElementById("playbutton").style.display = "none";

       document.getElementById("playbutton").style.display = "";

       document.getElementById("pausebutton").style.display = "none";

	}	 

   function anfang()
    {
       document.getElementById(ele1.id).currentTime = 0;
    }
</script>
<script language="JavaScript" async>  
 function change(form,action,count1)  
 {
  var mode='';

  for (var i = 0; i <= count1; i++){

  	if (document.getElementById('deutsch'+i).style.visibility=='')

  	{document.getElementById('deutsch'+i).style.visibility='visible';

	 mode='1'; 

	  }
  	if (document.getElementById('deutsch'+i).style.visibility=='visible') {

        document.getElementById('deutsch'+i).style.visibility='hidden';

   }else{document.getElementById('deutsch'+i).style.visibility='visible';}
 }	
} 
function change2(form,action,count1)  
 {
  var mode='';
  for (var i = 0; i <= count1; i++){
  	if (document.getElementById('franz'+i).style.visibility=='')
  	{document.getElementById('franz'+i).style.visibility='visible';
	 mode='1'; 
	  }
  	if (document.getElementById('franz'+i).style.visibility=='visible') {
        document.getElementById('franz'+i).style.visibility='hidden';
   }else{document.getElementById('franz'+i).style.visibility='visible';}
 }	
}
 </script>

